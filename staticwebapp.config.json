{
    "navigationFallback": {
        "rewrite": "/contact.html"
      },
    "routes":[

        {
            "route": "/",
            "methods": ["POST"],
            "redirect": "/about.html"
        },

        {
          "route": "/portfolio.html",
          "methods": ["POST", "PUT", "PATCH"]
        }
    ],
    "globalHeaders": {
        "Access-Control-Allow-Methods": "POST, GET, OPTIONS",
        "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
      },
      "mimeTypes": {
        ".json": "text/json"
      }
}