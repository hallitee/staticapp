{
    "routes":[
 
        {
            "route": "/*",
            "methods": ["POST"],
            "redirect": "/services.html"
        },
        {
            "route": "/",
            "methods": ["POST"],
            "redirect": "/services.html"
        },
        {
          "route": "/index.html",
          "methods": ["POST", "GET"],
          "redirect": "/about.html"
      },
        {
          "route": "/portfolio.html",
          "allowedRoles": ["authenticated"],
          "methods": ["POST", "PUT", "PATCH"]
        }
    ]
}