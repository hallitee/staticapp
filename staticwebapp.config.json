{
  "globalHeaders": {
    "Access-Control-Allow-Origin": "https://example.com",
    "Access-Control-Allow-Methods": "POST, PUT, PATCH"
  },
    "routes":[
 
        {
            "route": "/*",
            "allowedRoles": ["authenticated"],
            "methods": ["POST", "GET"],
            "redirect": "/services.html"
        },
        {
            "route": "/",
            "allowedRoles": ["authenticated"],
            "methods": ["POST","GET"],
            "redirect": "/services.html"
        },
        {
          "route": "/index.html",
          "methods": ["POST", "GET"],
          "redirect": "/blog.html"
      },
      {
        "route": "/contact.html",
        "methods": ["POST", "PUT", "PATCH"],
        "redirect": "/blog.html"
    },
        {
          "route": "/portfolio.html",
          "allowedRoles": ["authenticated"],
          "methods": ["POST", "PUT", "PATCH"]
        }
    ]
}